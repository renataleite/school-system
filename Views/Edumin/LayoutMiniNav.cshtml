@{
	Layout = "~/Views/Shared/_LayoutEdumin.cshtml";
	ViewData["Title"] = "<<Put your different title string value here for each page>>";
}
<div class="content-body">
    <div class="container-fluid">
        <div class="row page-titles mx-0">
            <div class="col-sm-6 p-md-0">
                <div class="welcome-text">
                    <h4>Hi, welcome back!</h4>
                    <p class="mb-0">Your business dashboard template</p>
                </div>
            </div>
            <div class="col-sm-6 p-md-0 justify-content-sm-end mt-2 mt-sm-0 d-flex">
                <ol class="breadcrumb">
                    <li class="breadcrumb-item"><a href="javascript:void(0)">Layout</a></li>
                    <li class="breadcrumb-item active"><a href="javascript:void(0)">Mini Navigation</a></li>
                </ol>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-12">
                <div class="card">
                    <div class="card-body">
                        <p>To make your dashboard navigation mini, please go to <code>dlabnav.js</code> file and make sure <code>sidebarStyle: "mini"</code> </p>
                        <pre>
                        <code class="javascript">
        (function($) {
            "use strict"

            new dlabSettings({
                sidebarStyle: "mini"
            });

        })(jQuery);
    </code>
</pre>

                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
@section Edumin_style
{
	<link rel="stylesheet" href="~/edumin/vendor/highlightjs/styles/darcula.css">
}

@section Edumin_script
{
    <script src="./vendor/global/global.min.js"></script>
    <script src="./vendor/bootstrap-select/dist/js/bootstrap-select.min.js"></script>
    <script src="./js/custom.min.js"></script>
    <script src="./js/dlabnav-init.js"></script>

    <script src="./vendor/highlightjs/highlight.pack.min.js"></script>
    <!-- Circle progress -->
    <script src="./js/layout-mini-nav.js"></script>

    <!-- Svganimation scripts -->
    <script src="./vendor/svganimation/vivus.min.js"></script>
    <script src="./vendor/svganimation/svg.animation.js"></script>
    
    
}